{{#each metaTags as |data attr|}}
  {{#if (isString this)}}
<meta {{attr}}="{{data}}">
  {{else}}
    {{#each this}}
<meta {{attr}}="{{name}}" content="{{{content}}}">
    {{/each}}
  {{/if}}
{{/each}}
<title>{{title}}</title>
<script>
  document.write('<script src="http://' + (location.host || 'localhost').split(':')[0] +
  ':35729/livereload.js?snipver=1"></' + 'script>')
</script> 
<base href="/" />